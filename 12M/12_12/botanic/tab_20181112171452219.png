	<script type="text/javascript">
	var input_text = "";
	var d = new Date();
	var year = d.getFullYear();
	var month =  (d.getMonth()+1);
	var dayLabel = d.getDay();
	var week = new Array('일', '월', '화', '수', '목', '금', '토');
	var todayLabel = week[dayLabel];
	var nextLabel1 = week[(dayLabel+1)%7];
	var nextLabel2 = week[(dayLabel+2)%7];
	
	if(month < 10){
		month = "0"+month;
	}
	var date =  d.getDate();
	if(date < 10){
		date = "0"+date;
	}
	var day = year+"-"+month+"-"+date; // 날짜
	var display_day = month+"."+date;
	
	
	$(document).ready(function(){
		function getWeather(){
			var d0, d1, d2;
			$.ajax({
				url:"/front/kr/board/getWeather.do",
				type:"POST",
				dataType:"xml",
				success:function(data){
					var data = $(data).find('data');
					$(data).each(function(){
						var time = $(this).children().eq(0).text();//시간
						var day = $(this).children().eq(1).text();//날짜
						var temp = $(this).children().eq(2).text();//온도
						var wfKor = $(this).children().eq(7).text();//날씨
						var wf_img = "";
						if(wfKor == "맑음"){
							wf_img = "icon_weather_01";
						}else if(wfKor == "구름 조금"){
							wf_img = "icon_weather_02";
						}else if(wfKor == "구름 많음"){
							wf_img = "icon_weather_03";
						}else if(wfKor == "흐림"){
							wf_img = "icon_weather_04";
						}else if(wfKor == "비"){
							wf_img = "icon_weather_05";
						}else if(wfKor == "눈/비"){
							wf_img = "icon_weather_06";
						}else if(wfKor == "눈"){
							wf_img = "icon_weather_07";
						}
						if(!d0 && day=='0'){
							d0 = "1";
							$(".today").append(display_day +" " +todayLabel+" " + "<img src=\"/front/img/"+wf_img+".png\" alt=\""+wfKor+"\"/>" + " " + temp+".℃");
						}
						if(time=='12'){
							if(!d1 && day=='1'){
								d1 = "<img src=\"/front/img/"+wf_img+".png\" alt=\""+wfKor+"\"/>";
								$("#nextWeather").append("<li>"+nextLabel1+" "+d1+"</li>");
							}else if(!d2 && day=='2'){
								d2 = "<img src=\"/front/img/"+wf_img+".png\" alt=\""+wfKor+"\"/>";
								$("#nextWeather").append("<li>"+nextLabel2+" "+d2+"</li>");
							}
						}
					});
				},error:function(request,status,error){
				}
			});
			getDust();
		}
		//종로기준 최근시간 미세지농도
		
		function getDust(){
			$.ajax({
				url:"/front/kr/board/getDust.do",
				type:"POST",
				dataType:"xml",
				success:function(data){
					var item = $(data).find('items').children().eq(0);

					$(".dust").append(" 미세먼지 "+ $(item).children().eq(6).text()+" ㎍/㎥");
					
				},error:function(request,status,error){
					//console.log("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
				}
			});
		}
		getWeather();
	});

	
function getSearchList(){
		
	
		 //검색 관련 script
		 var where_srch = "";
		 
		var from = "&from=yeyak_services.yeyak_services"; // from
		var key = "fe7e725bfce74fb5b310c8600e93075d";
		var url = "http://newsearch.seoul.go.kr/ksearch/searchJson.do?sno="+key+"&select=*"+from;
		var offset = "&offset=0&limit=10"; // 페이징 관련
		var where_park = "";
		var where_str = "";
			
		var where_park = " (TELNO='02-2104-9785' allword OR TELNO='02-2104-9785, 9790' allword OR TELNO='02-2104-9752' allword OR TELNO='02-2104-9796' allword OR TELNO='02-2104-9797' allword OR TELNO='02-2104-9787' allword) ";
		 //var offset = "&offset="+offset_val+"&limit="+limit; // 페이징 관련 
		var where = "&where="+encodeURIComponent(where_park+" and USEENDDATE >= '"+day+"' "+where_str+" AND MAXCLASSNM='교육' order by USEENDDATE ASC, USEBGNDATE ASC")+offset;
		var input_append = "";
		$.ajax({
				type: "GET",
				url: url+where, 
				success: function(html){
					totalNum = html.result.total_count;
					if(html.result.rows.length > 0){
					 for(var i=0; i<html.result.rows.length; i ++){ 
						 var input_append ="";
						 var result = html.result.rows;
						 input_append += " <li>";
						 input_append += " 		<a href=\""+result[i].fields.SVCURL+"\" target=\"_blank\">";
						 input_append += "			<span class=\"time\"><i></i></span>"; // 시작시간없음
						 input_append += "			<span class=\"txt\">"+result[i].fields.SVCNM+"</span>"; // 링크주소
						 input_append += " 		</a>";
						 input_append += "	</li>"; // 썸네일 이미지
						 $("#today_pg").append(input_append);
					 }
					}
				}
			}); 
	 }
	 </script>
	 
